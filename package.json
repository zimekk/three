{
  "name": "three",
  "version": "1.0.0",
  "repository": "https://github.com/zimekk/three.git",
  "author": "Marcin Makarewicz <zimekk@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "^12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint --edit \"$1\""
    }
  },
  "lint-staged": {},
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "scripts": {
    "clean": "rm -rf packages/*/public packages/*/lib",
    "build": "yarn workspaces run build",
    "serve": "yarn workspace @dev/app serve",
    "start": "yarn workspace @dev/app start",
    "update": "yarn upgrade-interactive --latest"
  },
  "workspaces": [
    "packages/*"
  ],
  "resolutions": {
    "@react-spring/three": "9.2.1",
    "@react-three/cannon": "2.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "fibers": "^5.0.0",
    "husky": "^4.3.7",
    "lerna": "^4.0.0",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.1",
    "type-coverage": "^2.17.5",
    "yarn-deduplicate": "^3.1.0"
  }
}
